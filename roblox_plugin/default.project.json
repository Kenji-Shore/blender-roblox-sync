{
	"name": "blender_roblox_sync",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/.bin",
		"**/.pnpm",
		"**/node_modules/typescript",
		"**/*.yaml"
	],
	"tree": {
		"$path": "out",
		"include": {
			"$path": "include",
			"node_modules": {
				"$path": "node_modules"
			}
		}
	}
}
